{
 "compilerOptions": {
  "target": "ES2022",
  "lib": ["ES2022"],
  "module": "ES2022",
  "moduleResolution": "Node",
  "outDir": "./dist",
  "rootDir": "./",
  "resolveJsonModule": true,
  "strict": true,
  "strictNullChecks": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "skipDefaultLibCheck": true,
  "skipLibCheck": true,
  "baseUrl": "./",
  "paths": {
   "@/*": ["./*"]
  }
 },
 "include": ["*.ts"],
 "exclude": ["node_modules", "dist"]
}
